function logOverlayAttribution(vaultId, sponsor, emotion) {
  fetch("/webhook/vault-preview", {
    method: "POST",
    headers: { "Content-Type": "application/json" },
    body: JSON.stringify({
      vaultId,
      sponsor,
      emotion,
      amplifiers: ["pika.art", "upscale.media", "make.com"],
      timestamp: Date.now()
    })
  });
}
function logScrollOverlay(scrollId, emotion, amplifiers) {
  fetch("/webhook/scroll-overlay", {
    method: "POST",
    headers: { "Content-Type": "application/json" },
    body: JSON.stringify({
      scrollId,
      emotion,
      overlay: "https://www.youtube.com/@charlesbuchanan-d1e",
      amplifiers,
      timestamp: Date.now()
    })
  });
}
function logSignalOverlay(signalId, emotion, assets) {
  fetch("/webhook/signal-overlay", {
    method: "POST",
    headers: { "Content-Type": "application/json" },
    body: JSON.stringify({
      signalId,
      emotion,
      assets,
      overlay: "https://www.youtube.com/@charlesbuchanan-d1e",
      amplifiers: ["pika.art", "upscale.media", "make.com"],
      timestamp: Date.now()
    })
  });
}
