function triggerOverlay(el) {
  const payload = {
    contributor: el.dataset.contributor,
    emotion: el.dataset.emotion,
    preview: el.dataset.preview,
    timestamp: Date.now()
  };

  fetch("/webhook/overlay", {
    method: "POST",
    headers: { "Content-Type": "application/json" },
    body: JSON.stringify(payload)
  });

  window.open(payload.preview, "_blank");
}
function triggerTrafficOverlay(el) {
  const payload = {
    emotion: el.dataset.emotion,
    preview: el.dataset.preview,
    affiliate: el.dataset.affiliate,
    timestamp: Date.now()
  };

  fetch("/webhook/traffic-overlay", {
    method: "POST",
    headers: { "Content-Type": "application/json" },
    body: JSON.stringify(payload)
  });

  window.open(payload.preview, "_blank");
  window.open(payload.affiliate, "_blank");
}
function triggerAffiliateVault(el) {
  const payload = {
    emotion: el.dataset.emotion,
    affiliate: el.dataset.affiliate,
    sponsor: el.dataset.sponsor,
    timestamp: Date.now()
  };

  fetch("/webhook/affiliate-vault", {
    method: "POST",
    headers: { "Content-Type": "application/json" },
    body: JSON.stringify(payload)
  });

  window.open(payload.affiliate, "_blank");
}
