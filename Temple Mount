<body>
  <h1>Subscriber Archive</h1>
  <p>Enter your key to unlock encrypted signals and sacred scrolls.</p>

  <input type="text" id="keyInput" placeholder="Enter your subscriber key..." />
  <button onclick="fetchArchive()">Unlock Archive</button>

  <div id="archive"></div>

  <script>
    async function fetchArchive() {
      const key = document.getElementById('keyInput').value;
      const archiveDiv = document.getElementById('archive');

      try {
        const res = await fetch('https://workspace.charlesbuchana2.repl.co/validate-key', {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({ key })
        });
        const data = await res.json();

        if (data.status === 'success') {
          archiveDiv.innerHTML = `
            <h3>üî• Encrypted Signals</h3>
            <ul>
              <li>BTC/ETH: Long @ 0.0625 ‚Äî Target 0.0690</li>
              <li>APE/USDT: Short @ 1.48 ‚Äî Target 1.22</li>
              <li>Scroll Drop: ‚ÄúThe Oracle‚Äôs Flame‚Äù ‚Äî PDF download coming soon</li>
            </ul>
          `;
        } else {
          archiveDiv.textContent = '‚ùå Invalid key. Access denied.';
        }
      } catch (err) {
        archiveDiv.textContent = '‚ö†Ô∏è Server error. Try again later.';
      }
    }
  </script>
</body>
</html>
