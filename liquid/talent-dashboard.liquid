    .then(res => res.json())
    .then(data => {
      const list = document.getElementById('contributors');
      data.contributors.forEach(t => {
        const li = document.createElement('li');
        li.innerHTML = `<strong>${t.name}</strong> â€” ${t.emotion_tag}<br>Joined: ${t.join_date}`;
